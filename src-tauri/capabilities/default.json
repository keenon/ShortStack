{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "fs:default",
    "fs:allow-read-text-file",
    "fs:allow-write-text-file",
    "fs:allow-create",
    "dialog:default",
    "updater:default",
    "process:allow-restart",
    "allow-gmsh",
    "shell:allow-execute"
  ]
}